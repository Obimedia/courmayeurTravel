{# Login Unbranded #}
<form class="login-form" method="post" action="{{ path('user_login_check') }}">
    <div class="form-group">
        <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Email" required="required" name="email">
    </div>
    <div class="form-group">
        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" required="required" name="password">
    </div>
    <button class="btn btn-primary" type="submit">Accedi</button>
    <a href="{{ path('user_register') }}">Non sei registrato? Registrati!</a>
    <a href="{{ path('user_resend_password') }}">Hai dimenticato la password?</a>
</form>
